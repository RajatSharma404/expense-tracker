{
  "name": "expense-tracker",
  "version": "1.0.0",
  "description": "Full-featured expense tracking application",
  "scripts": {
    "dev": "concurrently -n \"backend,frontend\" -c \"cyan,magenta\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:b": "npm --prefix backend run dev",
    "dev:f": "npm --prefix frontend run dev",
    "dev:no-mongo": "concurrently -n \"backend,frontend\" -c \"yellow,blue\" \"npm run dev:backend:no-mongo\" \"npm run dev:frontend\"",
    "dev:backend:no-mongo": "npm --prefix backend run dev",
    "install:all": "npm install && npm install --prefix backend && npm install --prefix frontend",
    "build": "npm --prefix backend run build && npm --prefix frontend run build",
    "start": "npm --prefix backend start"
  },
  "keywords": [
    "expense",
    "tracker",
    "finance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}